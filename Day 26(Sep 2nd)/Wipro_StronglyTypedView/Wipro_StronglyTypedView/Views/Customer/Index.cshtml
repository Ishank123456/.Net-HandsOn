@model IEnumerable<Wipro_StronglyTypedView.Models.Customer>

<html>
    <head>
        <title>Customer Table Page</title>
    </head>

    <body>
        <h1 style="text-align:center">Customer Table</h1>
        <hr/>

        <table class="table table-striped">
            <tr>
                <th>Customer ID</th>
                <th>Customer Name</th>
                <th>Customer Address</th>
                <th>Customer Contact Number</th>
            </tr>

            @{
                foreach(var item in Model)
                {
                    <tr>
                        <td>@item.CustId</td>
                        <td>@item.CustName</td>
                        <td>@item.CustAddress</td>
                        <td>@item.CustContactNo</td>
                    </tr>
                }
            }
        </table>
    </body>
</html>
